<% end %>
</td>
<td>
<% if l.excellent_cases != 0 %>
<% @excellent = (l.excellent_cases/l.cases_attempted.to_f)*100.00 %>
<%= sprintf('%.2f', @excellent) %>%
<% else %>
-
<% end %>
</td>
</tr>
<% end %>
